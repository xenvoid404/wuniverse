[
    {
        "id": "deploy-wuniverse",
        "execute-command": "/var/www/wuniverse/deploy/run.sh",
        "command-working-directory": "/var/www/wuniverse",
        "response-message": "Deployment triggered successfully",
        "trigger-rule": {
            "and": [
                {
                    "match": {
                        "type": "value",
                        "value": "refs/heads/master",
                        "parameter": {
                            "source": "payload",
                            "name": "ref"
                        }
                    }
                },
                {
                    "match": {
                        "type": "value",
                        "value": "push",
                        "parameter": {
                            "source": "header",
                            "name": "X-GitHub-Event"
                        }
                    }
                }
            ]
        },
        "pass-arguments-to-command": [
            {
                "source": "payload",
                "name": "head_commit.id"
            },
            {
                "source": "payload",
                "name": "pusher.name"
            }
        ],
        "pass-environment-to-command": [
            {
                "envname": "WEBHOOK_COMMIT_ID",
                "source": "payload",
                "name": "head_commit.id"
            }
        ]
    }
]
